##
##  Makefile -- Build procedure for sample memc_sess Apache module
##  Autogenerated via ``apxs -n memc_sess -g''.
##

APXS=@APXS@
APACHECTL=@APACHECTL@
LIBS=-lmemcached

all: mod_memc_sess.c
	$(APXS) $(LIBS) -c mod_memc_sess.c

install: 
	$(APXS) $(LIBS) -i -c mod_memc_sess.c

clean:
	-rm -f mod_memc_sess.o mod_memc_sess.lo mod_memc_sess.slo mod_memc_sess.la 

reload: install restart

start:
	$(APACHECTL) start

restart:
	$(APACHECTL) restart

stop:
	$(APACHECTL) stop

